{
    "name": "final-form-storage",
    "version": "0.1.0",
    "description": "A set of subscribers and mutators helping persist the state of form based on Final Form",
    "repository": {
        "type": "github",
        "url": "https://github.com/phoulgaux/final-form-storage"
    },
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "clean": "rimraf dist",
        "lint": "tslint --project .",
        "test": "jest",
        "typecheck": "tsc --noEmit"
    },
    "author": "Piotr Balbier <oss@balbier.net>",
    "license": "MIT",
    "devDependencies": {
        "@types/jest": "^23.3.1",
        "@types/lodash": "^4.14.116",
        "final-form": "^4.20.1",
        "jest": "^23.4.2",
        "jsverify": "^0.8.3",
        "lodash": "^4.17.10",
        "rimraf": "^2.6.2",
        "source-map-support": "^0.5.6",
        "ts-jest": "^23.1.2",
        "tslint": "^5.11.0",
        "tslint-config-prettier": "^1.14.0",
        "typemoq": "^2.1.0",
        "typescript": "^3.0.1"
    },
    "peerDependencies": {
        "final-form": "^4.20.1"
    },
    "jest": {
        "collectCoverage": true,
        "coverageReporters": [
            "lcov",
            "html"
        ],
        "moduleFileExtensions": [
            "ts",
            "js"
        ],
        "testRegex": "spec\\.ts$",
        "testURL": "http://localhost/",
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        }
    }
}
